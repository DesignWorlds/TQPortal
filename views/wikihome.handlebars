<h1>Wiki Central!!</h1>
<div class="col-xs-11" position:"left";>
  <form id="newtopic" action="/newtopic" method="post">
    <div class="form-group">
      <label>Topic Title</label>
      <input type="text" class="form-control" name="title">
    </div>

    <button type="submit" class="btn btn-warning btn-lg">Submit</button>
  </form><br/><br/><hr>
  <div class="form-group">
     <table id="wikitable" class="display" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th width="70%">Subject</th><th>Author</th><th>Date</th>
        </tr>
      </thead>
      <tbody>
      </tbody>
    </table>
</div>

</div>