<h1>List Users and Edit credentials</h1>
<div class="col-xs-11" position:"left";>
 <form id="credentialform" action="/selectuser" method="get">
  <div class="form-group">
   <label>Email for Selected User</label>
   <input type="text" class="form-control" name="email">
  </div>
  <button type="submit" class="btn btn-warning btn-lg">Select</button>
 </form> <br/><hr>
<div class="col-xs-11 pre-scrollable" position:"left"; style="border: 1px solid #e1e1e8;">
 <table  cellspacing="0" width="100%" >
      <thead>
        <tr><th>Email</th><th>Handle</th><th>Full Name</th><th>Credentials</th></tr>
      </thead>
      <tbody>
      {{#each usrtable}}
      <tr><td><a href="/usr/{{handle}}">{{email}}</a></td><td>{{handle}}</td><td>{{name}}</td><td>{{credentials}}</td></tr>
      {{/each}}
      </tbody>

 </table>

</div>
